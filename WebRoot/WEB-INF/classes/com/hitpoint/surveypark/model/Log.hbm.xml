<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hitpoint.surveypark.model">
	<class name="Log" table="logs">
		<id name="id" column="id" type="integer">
			<generator class="identity"/>
		</id>	
		<property name="operator" column="operator" type="string" length="100"/>
		<property name="operateName" column="operatename" type="string" length="100"/>
		<property name="operateParame" column="operateparame" type="string" length="100"/>
		<property name="operateResult" column="operateresult" type="string" length="100"/>
		<property name="resultMsg" column="resultmsg" type="string" length="100"/>
		<property name="operateTime" column="operatetime" type="timestamp"/>
		
		
	</class>
</hibernate-mapping>